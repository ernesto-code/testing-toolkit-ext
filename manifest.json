{
  "manifest_version": 3,
  "name": "A11y Toolkit",
  "version": "1.0",
  "description": "Toolkit ",
  "action": {
    "default_title": "A11y testing Toolkit"
  },
  "icons": {
    "16": "images/toolkit-icon-16.png",
    "32": "images/toolkit-icon-32.png",
    "48": "images/toolkit-icon-48.png"
  },
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "scripts/background.js"
  }
}